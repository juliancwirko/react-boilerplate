  "dependencies": {
    "axios": "~0.7.0",
    "lodash": "~3.10.1",
    "react": "~0.14.3",
    "react-dom": "~0.14.3",
    "react-router": "~1.0.0",
    "rupture": "~0.6.1",
    "s-grid": "~1.1.2",
    "history": "~1.13.1"
