  "bugs": {
    "url": "https://github.com/juliancwirko/react-boilerplate/issues"
  },
  "homepage": "https://github.com/juliancwirko/react-boilerplate",
  "dependencies": {
    "history": "~2.1.1",
    "lodash": "~4.11.2",
    "react": "~15.0.2",
    "react-dom": "~15.0.2",
    "react-redux": "^4.4.5",
    "react-router": "~2.4.0",
    "redux": "^3.5.2",
    "rupture": "~0.6.1",
    "s-grid": "~1.1.2"
  },
  "devDependencies": {
    "autoprefixer": "~6.3.6",
